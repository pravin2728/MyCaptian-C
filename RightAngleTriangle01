#include<stdio.h>
#include<conio.h>
void main()
{
    int n,i,j;
    printf("Enter no of rows to be printed as right angle triangle:\n");
    scanf("%d",&n);
    for(i=0;i<=n;i++)
        {
            for(j=0;j<=i;j++)
                {
                    printf("01");
                }
        printf("\n");
        }
}
